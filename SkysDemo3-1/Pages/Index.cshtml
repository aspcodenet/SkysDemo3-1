@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<table class="table">
    <thead>
    <tr><th>
            Namn
        </th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.Items)
    {
        <tr>
            <td>@item.Namn</td>
            <td><a asp-page="/Category" asp-route-categoryId="@item.Id" class="btn btn-primary">View</a></td>
        </tr>
    }
    </tbody>
</table>


